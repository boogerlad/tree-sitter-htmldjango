{
  "grammars": [
    {
      "name": "htmldjango",
      "camelcase": "HTMLDjango",
      "scope": "text.html.django",
      "path": ".",
      "external-files": [
        "src/tag.h"
      ],
      "file-types": [
        "html",
        "htm",
        "django",
        "htmldjango"
      ],
      "highlights": "queries/highlights.scm",
      "injections": "queries/injections.scm",
      "injection-regex": "^(html|htmldjango|django)$"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "HTML + Django template grammar for tree-sitter",
    "authors": [
      {
        "name": "boogerlad"
      }
    ],
    "links": {
      "repository": "https://github.com/boogerlad/tree-sitter-htmldjango"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true
  }
}
